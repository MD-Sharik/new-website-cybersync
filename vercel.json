{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "MONGODB_URI": "@MONGODB_URI",
    "SMTP_HOST": "@SMTP_HOST",
    "SMTP_PORT": "@SMTP_PORT",
    "SMTP_USER": "@SMTP_USER",
    "SMTP_PASS": "@SMTP_PASS",
    "SMTP_FROM": "@SMTP_FROM",
    "CONTACT_EMAIL": "@CONTACT_EMAIL",
    "GEMINI_API_KEY": "@GEMINI_API_KEY"
  }
}
